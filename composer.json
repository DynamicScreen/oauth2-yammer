{
    "name": "upro/oauth2-yammer",
    "description": "Yammer OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "U Pro",
            "homepage": "https://github.com/upro"
        }
